<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="regression test" verbose="1">
    <groups>
        <run>
            <include name="Regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.orangehrmlive.demo.customlisterners.CustomListeners"></listener>
    </listeners>
    <test name="test">
        <classes>
            <class name="com.orangehrmlive.demo.testsuite.UsersTest"></class>
            <class name="com.orangehrmlive.demo.testsuite.LoginTest"></class>
        </classes>
    </test>
</suite>
